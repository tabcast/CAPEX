<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" xmlns:footerbar="sap.ushell.ui.footerbar" xmlns:f="sap.ui.layout.form" controllerName="co.com.postobon.controller.Detail">
	<semantic:DetailPage id="page" navButtonPress="onNavBack" showNavButton="{device>/system/phone}" title="{i18n>detailTitle}" busy="{detailView>/busy}" busyIndicatorDelay="{detailView>/delay}" floatingFooter="true">
		<semantic:content>
			<Panel expandable="true" expanded="true" width="auto" class="sapUiResponsiveMargin">
				<headerToolbar>
					<OverflowToolbar>
						<Title text="Datos Solicitud"/>
					</OverflowToolbar>
				</headerToolbar>
					<!--<VBox class="sapUiSmallMargin">-->
					<f:SimpleForm id="SimpleFormDisplay471" editable="false" layout="ResponsiveGridLayout" title="" labelSpanXL="4" labelSpanL="4" labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="1" emptySpanL="1" emptySpanM="1" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false">
						<f:content>
							<Label text="Solicitud"/>
							<Text id="Solicitud" text="{solicitud}"/>
							<Label text="Sociedad"/>
							<Text id="Sociedad" text="{bukrs} - {butxt}"/>		
							<Label text="Centro"/>
							<Text id="Centro" text="{ubicacion}-{ubicacion_nombre}"/>	
							<Label text="Responsable"/>
							<Text id="Responsable" text="{responsable} - {responsable_nombre}"/>	
							<Label text="Valor Neto"/>
							<Text id="ValorNeto" text="{valor_neto}"/>
							<Label text="Valor Aprobado"/>
							<Text id="ValorAprobado" text="{valor_pre_aprobado}"/>	
							<Label text="Estado"/>
							<Text id="Estado" text="{estado} - {estado_desc}"/>								
						</f:content>
					</f:SimpleForm>
					<!--</VBox>-->				
			</Panel>
			<!--<ObjectHeader id="objectHeader" title="{solicitud}" numberUnit="{waers}"></ObjectHeader>-->
			<IconTabBar id="iconTabBar" class="sapUiResponsiveContentPadding">
				<items>
					<IconTabFilter id="iconTabBarFilter0" icon="sap-icon://message-information" tooltip="{i18n>detailIconTabBarInfo}" text="{i18n>tab_info}">
						<Label text="111111111"/>
						
			<Table id="lineItemsList" width="auto" updateFinished="onListUpdateFinished" noDataText="{i18n>detailLineItemTableNoDataText}" busyIndicatorDelay="{detailView>/lineItemTableDelay}" class="sapUiResponsiveMargin">
				<headerToolbar>
					<Toolbar id="lineItemsToolbar">
						<Title id="lineItemsHeader" text="{detailView>/lineItemListTitle}"/>
					</Toolbar>
				</headerToolbar>
				<infoToolbar>
					<OverflowToolbar>
						<Label text="Wide range of available products"/>
					</OverflowToolbar>
				</infoToolbar>				
				<columns>
					<Column>
						<Text text="{i18n>detailLineItemTableIDColumn}"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Right">
						<Text text="{i18n>detailLineItemTableUnitNumberColumn}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<cells>
							<ObjectIdentifier title="{responsable}" text="{bukrs}"/>
							<ObjectNumber unit="{bukrs}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>						
						
					</IconTabFilter>
					<IconTabFilter id="iconTabBarF6" icon="sap-icon://attachment-text-file" tooltip="{i18n>tab_textos}" text="{i18n>tab_textos}">
						<Label text="666733"/>
					</IconTabFilter>			
					<IconTabFilter id="iconTabBarFilter1" icon="sap-icon://numbered-text" tooltip="{i18n>detailIconTabBarInfo}" text="{i18n>tab_item}" iconColor="Positive" count="23-{total_items}">
					
						<Label text="222"/>
						<Table noDataText="Drop column list items here and columns in the area above" id="tbl_items">
