text="{i18n>tblitems_Orden}"
text="{i18n>tblitems_ActFijo}"
text="{i18n>tblitems_Observ}"
						        
						        
						        </columns></Table>
					<content/>
					</IconTabFilter>
					<IconTabFilter id="iconTabBarFilter2" icon="sap-icon://account" tooltip="{i18n>tab_apro}" text="{i18n>tab_apro}">
						<Table noDataText="Drop column list items here and columns in the area above" id="_table_aprob">
						    <items>
						        <ColumnListItem id="__item3">
						            <cells>
						                <Text text="Row 1 Cell 1" id="__text10"/>
						                <Text text="Row 1 Cell 2" id="__text11"/>
						                <Text text="Row 1 Cell 3" id="__text12"/></cells>
						        </ColumnListItem>
						        <ColumnListItem id="__item4">
						            <cells>
						                <Text text="Row 2 Cell 1" id="__text13"/>
						                <Text text="Row 2 Cell 2" id="__text14"/>
						                <Text text="Row 2 Cell 3" id="__text15"/></cells>
						        </ColumnListItem></items>
						    <columns>
						        <Column id="__column5">
						            <header>
						                <Label text="Header 1" id="__label15"/>
						            </header>
						        </Column>
						        <Column id="__column6">
						            <header>
						                <Label text="Header 2" id="__label16"/>
						            </header>
						        </Column>
						        <Column id="__column7">
						            <header>
						                <Label text="Header 3" id="__label17"/>
						            </header>
						        </Column></columns></Table>
					<content/>
					</IconTabFilter>
					<IconTabFilter id="iconTabBarFilter3" icon="sap-icon://documents" tooltip="{i18n>tab_ampl}" text="{i18n>tab_ampl}">
						<Label text="444"/>
					</IconTabFilter>
					<IconTabFilter id="iconTabBarFilter" icon="sap-icon://vds-file" tooltip="{i18n>tab_afin}" text="{i18n>tab_afin}">
						<Label text="555"/>
					</IconTabFilter>
				</items>
			</IconTabBar>

		
		</semantic:content>

		<semantic:sendEmailAction>
			<semantic:SendEmailAction id="shareEmail" press="onShareEmailPress"/>
		</semantic:sendEmailAction>
		<semantic:shareInJamAction>
			<semantic:ShareInJamAction id="shareInJam" visible="{FLP>/isShareInJamActive}" press="onShareInJamPress"/>
		</semantic:shareInJamAction>
		<semantic:saveAsTileAction>
			<footerbar:AddBookmarkButton id="shareTile" title="testttt{detailView>/saveAsTileTitle}">

			</footerbar:AddBookmarkButton>
		</semantic:saveAsTileAction>
	</semantic:DetailPage>
</mvc:View>