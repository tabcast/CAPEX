						        </ColumnListItem></items>
						    <columns>
						        <Column id="tbl_items_items">
						            <header>
						                <Label text="{i18n>tblitems_items}"  id="__label12"/>
						            </header>
						        </Column>
						        <Column id="__column3">
						            <header>
						                <Label text="{i18n>tblitems_canti}"  id="__label13"/>
						            </header>
						        </Column>
						        <Column id="__column4">
						            <header>
						                <Label text="{i18n>tblitems_zonapro}" id="__label14"/>
						            </header>
						        </Column>
						        
              					<Column id="col_tblitems_desczonapro">
						            <header>
						                <Label text="{i18n>tblitems_desczonapro}" id="lbl_tblitems_desczonapro"/>
						            </header>
						        </Column>
              
            
text="{i18n>tblitems_desczonapro}"
text="{i18n>tblitems_inversion}"
text="{i18n>tblitems_moneda}"
text="{i18n>tblitems_TRM}"
